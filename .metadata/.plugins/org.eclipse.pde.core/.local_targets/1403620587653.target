<?xml version="1.0" encoding="UTF-8"?><?pde version="3.6"?><target name="Running Platform">
<locations>
<location path="${eclipse_home}" type="Profile"/>
</locations>
<environment>
<os>win32</os>
<ws>win32</ws>
<arch>x86</arch>
<nl>en_US</nl>
</environment>
<launcherArgs>
<vmArgs>-Xdump:system:events=systhrow,filter=java/lang/OutOfMemoryError,request=exclusive+prepwalk -Xms100m -Xmx512m -Dosgi.requiredJavaVersion=1.6 -Dosgi.bundlefile.limit=100</vmArgs>
</launcherArgs>
</target>